POSTGRES_USER=tessrax
POSTGRES_PASSWORD=password
POSTGRES_DB=tessrax_state
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

REDIS_HOST=redis
REDIS_PORT=6379

API_KEY=tessrax-os-key-123
TESSRAX_API_KEY=${API_KEY}

LEDGER_PATH=tessrax/ledger/ledger.jsonl
LEDGER_INDEX_PATH=tessrax/ledger/index.db

AION_MODEL_PATH=models/aion-7b.gguf
TESSRAX_AION_MODEL=${AION_MODEL_PATH}
AION_SOCKET=/tmp/aion.sock

TESSRAX_TARGET_URL=http://example.com

TESSRAX_DB_USER=${POSTGRES_USER}
TESSRAX_DB_PASSWORD=${POSTGRES_PASSWORD}
TESSRAX_DB_NAME=${POSTGRES_DB}
TESSRAX_DB_HOST=${POSTGRES_HOST}
TESSRAX_DB_PORT=${POSTGRES_PORT}
TESSRAX_DB_URL=postgresql://${TESSRAX_DB_USER}:${TESSRAX_DB_PASSWORD}@${TESSRAX_DB_HOST}:${TESSRAX_DB_PORT}/${TESSRAX_DB_NAME}

TESSRAX_REDIS_URL=redis://${REDIS_HOST}:${REDIS_PORT}/0
TESSRAX_CRAWLER_TIMEOUT=10
TESSRAX_CRAWLER_MAX_DEPTH=3
TESSRAX_CRAWLER_MAX_STATES=30
TESSRAX_HEARTBEAT=10
TESSRAX_MAX_ATTEMPTS=3
TESSRAX_POLL_INTERVAL=1
TESSRAX_IDLE_SLEEP=2
